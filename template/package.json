{
  "name": "{{ name }}",
  "version": "0.0.1",
  "description": "{{ description }}",
  "productName": "{{ productName }}",
  "cordovaId": "{{ cordovaId }}",
  "capacitorId": "{{ capacitorId }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    {{#preset.lint}}"lint": "eslint --ext .js,{{#preset.typescript}}.ts,{{/preset.typescript}}.vue --ignore-path .gitignore ./",{{/preset.lint}}
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    {{#if_eq typescriptConfig "composition"}}"@vue/composition-api": "^0.4.0",{{/if_eq}}
    {{#if_eq typescriptConfig "class"}}
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    {{/if_eq}}
    {{#preset.axios}}"axios": "^0.18.1",{{/preset.axios}}
    {{#preset.i18n}}"vue-i18n": "^8.0.0",{{/preset.i18n}}
    "@quasar/extras": "^1.0.0",
    {{#if preset.typescript}}
    "quasar": "^1.9.5"
    {{else}}
    "quasar": "^1.0.0"
    {{/if}}
  },
  "devDependencies": {
    {{#preset.lint}}
    "babel-eslint": "^10.0.1",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-vue": "^6.1.2",
    {{#if_eq lintConfig "standard"}}
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    {{/if_eq}}
    {{#if_eq lintConfig "airbnb"}}
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    {{/if_eq}}
    {{#if_eq lintConfig "prettier"}}
    "eslint-config-prettier": "^6.9.0",
    {{/if_eq}}
    {{/preset.lint}}
    {{#preset.typescript}}
    {{#preset.lint}}
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    {{/preset.lint}}
    "@types/node": "^10.17.15",
    {{/preset.typescript}}
    {{#if preset.typescript}}
    "@quasar/app": "^1.6.0"
    {{else}}
    "@quasar/app": "^1.0.0"
    {{/if}}
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]{{#preset.lint}},
  "resolutions": {
    "@babel/parser": "7.7.5"
  }
  {{/preset.lint}}
}
