{
  "name": "{{ name }}",
  "version": "0.0.1",
  "description": "{{ description }}",
  "productName": "{{ productName }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    {{#preset.lint}}"lint": "eslint --ext .js,{{#preset.typescript}}.ts,{{/preset.typescript}}.vue ./",{{/preset.lint}}
    "test": "echo \"No test specified\" && exit 0"
  },
  "dependencies": {
    {{#if_eq typescriptConfig "composition"}}"@vue/composition-api": "^1.0.0-rc.3",{{/if_eq}}
    {{#if_eq typescriptConfig "class"}}
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    {{/if_eq}}
    {{#preset.axios}}"axios": "^0.21.1",{{/preset.axios}}
    {{#preset.i18n}}"vue-i18n": "^8.0.0",{{/preset.i18n}}
    "@quasar/extras": "^1.0.0",
    "core-js": "^3.6.5",
    "quasar": "^1.0.0"
  },
  "devDependencies": {
    {{#preset.lint}}
    "babel-eslint": "^10.0.1",
    "eslint": "^7.21.0",
    {{#unless preset.typescript}}
    "eslint-webpack-plugin": "^2.4.0",
    {{/unless}}
    "eslint-plugin-vue": "^7.7.0",
    {{#if_eq lintConfig "standard"}}
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    {{/if_eq}}
    {{#if_eq lintConfig "airbnb"}}
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    {{/if_eq}}
    {{#if_eq lintConfig "prettier"}}
    "eslint-config-prettier": "^8.1.0",
    {{/if_eq}}
    {{/preset.lint}}
    {{#preset.typescript}}
    {{#preset.lint}}
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    {{/preset.lint}}
    "@types/node": "^10.17.15",
    {{/preset.typescript}}
    "@quasar/app": "^2.0.0"
  },
  "browserslist": [
    {{#preset.ie}}
    "ie >= 11",
    {{/preset.ie}}
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
